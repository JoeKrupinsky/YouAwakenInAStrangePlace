{
  "name": "you-awaken-in-a-strange-place",
  "version": "1.0.0",
  "description": "Companion app for the one-shot ttrpg You Awaken In A Strange Place",
  "private": false,
  "license": "MIT",
  "browserslist": [
    "last 7 versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "main": "/src/index.js",
  "prox": "http://localhost:4001",
  "scripts": {
    "build": "node ./buildScript",
    "build-front": "react-scripts build",
    "eject": "react-scripts eject",
    "start-server": "nodemon server/server.js --watch server/*",
    "start-front": "react-scripts start",
    "start": "concurrently \"npm run start-server\" \"npm run start-front\" --kill-others --kill-others-on-fail",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "axios": "^0.21.1",
    "body-parser": "1.19.0",
    "bootstrap": "3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "express": "4.17.1",
    "formik": "^2.2.6",
    "helmet": "3.22.0",
    "knex": "0.20.13",
    "react": "^17.0.2",
    "react-bootstrap": "^1.6.0",
    "react-dice-complete": "^1.2.1",
    "react-dom": "^17.0.2",
    "react-hot-toast": "^1.0.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "sqlite3": "^5.0.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/express": "4.17.6",
    "@types/react": "16.9.34",
    "@types/react-dom": "16.9.6",
    "concurrently": "5.1.0",
    "nodemon": "2.0.3",
    "react-scripts": "3.4.1",
    "sqlite3": "4.1.1",
    "typescript": "3.8.3"
  }
}
